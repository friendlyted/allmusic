<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <display-name>Слуховой анализ</display-name>

    <servlet>
        <servlet-name>intervalQuest</servlet-name>
        <servlet-class>pro.friendlyted.allmusic.servlet.IntervalQuestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>triadQuest</servlet-name>
        <servlet-class>pro.friendlyted.allmusic.servlet.TriadQuestServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>intervalQuest</servlet-name>
        <url-pattern>/quest/interval</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>triadQuest</servlet-name>
        <url-pattern>/quest/triad</url-pattern>
    </servlet-mapping>
</web-app>